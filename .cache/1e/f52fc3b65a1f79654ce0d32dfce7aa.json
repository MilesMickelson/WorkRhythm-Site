ery L, Snell Roundhand, Cursive;\n$primary-font: Verdana, Arial, Times, Courier, Georgia;\n\n@mixin header {\n\tcolor: $black-5;\n\tfont-family: $primary-font;\n}\n\n@mixin primary {\n\tcolor: $black-5;\n\tfont-family: $primary-font;\n}\n\n//-----ANIMATIONS-----\n\n//-----STYLING-----\n\n@mixin cursor {\n\tcursor: crosshair;\n}\n\n.visible {\n\tdisplay: normal;\n}\n\n.inVisible {\n\tdisplay: none;\n}\n\n.hidden {\n\tvisibility: hidden;\n}\n\n.center-wrap {\n\ttext-align: center;\n}\n\n/*#endregion Global Styling*/\n\n/*#region HTML Tag Styling*/\n\n* {\n\tbox-sizing: border-box;\n}\n\nhtml {\n  scroll-behavior: smooth;\n\tbackground-color: $white-5;\n  background-size: 1150px 100%;\n  -webkit-background-size: 1150px 100%;\n  -moz-background-size: 1150px 100%;\n  -o-background-size: 1150px 100%;\n  background-repeat: no-repeat;\n\tbackground-position: center center;\n  background-attachment: fixed;\n}\n\n//Header Wraps\nheader {\n  display: visible;\n}\n\n//Main Title \nh1 {\n\t@include header;\n\tfont-size: calc(3rem + 3vmin);\n\tmargin-block-start: 0;\n\tmargin-block-end: 0;\n\tmargin-inline-start: 0px;\n\tmargin-inline-end: 0px;\n\tmargin: 0;\n}\n\n//Sub Title\nh2 {\n  @include header;\n\tfont-size: calc(1.75rem + 2.5vmin);\n\tmargin-block-start: 0;\n\tmargin-block-end: 0;\n\tmargin-inline-start: 0px;\n\tmargin-inline-end: 0px;\n\tmargin: 0;\n}\n\n//Page Title\nh3 {\n\t@include header;\n\ttext-align: center;\n\tfont-size: calc(1.75rem + 2vmin);\n\ttext-decoration: underline;\n\tmargin: 25px 0 15px 0;\n}\n\n//Section Header\nh4 {\n  @include header;\n\tfont-size: calc(1.5rem + 1vmin);\n\ttext-decoration: underline;\n\ttext-align: center;\n\tmargin: 20px 0 0 0;\n}\n\n//Small Section Header\nh5 {\n  @include header;\n\tfont-size: calc(1.25rem + 1vmin);\n\ttext-decoration: underline;\n  text-align: center;\n\tmargin: 10px 0 0 0;\n}\n\n//Very Small Section Header\nh6 {\n  @include header;\n\tfont-size: calc(1rem + 1vmin);\n\ttext-decoration: underline;\n  text-align: center;\n\tmargin: 5px 0 0 0;\n}\n\np {\n\t@include primary;\n\tfont-size: calc(.9rem + .7vmin);\n\tfont-weight: regular;\n\ttext-align: left;\n\ttext-indent: 1.25em;\n\tword-break: break-word;\n\tdisplay: visible;\n  margin: 5px 3% 15px;\n\tmargin-block-start: 0;\n\tmargin-block-end: 0;\n\tmargin-inline-start: 0;\n\tmargin-inline-end: 0;\n}\n\nol {\n  list-style-type: decimal;\n  margin: 0;\n  padding: 0;\n}\n\nul {\n\tdisplay: visible;\n\t@include primary;\n\tlist-style-type: arrows;\n  padding: 0;\n  padding-left: 40px;\n\tmargin: 0 0 0 30px;\n}\n\nli {\n  display: list-item;\n\tmargin-top: 10px;\n}\n\nem {\n\t@include primary;\n\tfont-style: normal;\n}\n\nlabel {\n\t@include primary;\n}\n\na {\n\t@include primary;\n\tfont-size: calc(.9rem + .7vmin);\n  text-decoration: underline;\n}\n\na:hover {\n\t@include cursor;\n}\n\ndetails {\n  display: block;\n}\n\nsummary {\n\tfont-size: calc(1.5rem + 1vmin);\n\ttext-decoration: underline;\n\tmargin: 15px 0 0 0;\n}\n\n/*#endregion HTML Tag Styling*/\n\n/*#region Title*/\n\n/*#endregion Title*/\n\n/*#region NavBar*/\n\n/*#endregion NavBar*/\n\n/*#region Page1*/\n\n/*#endregion Page1*/\n\n/*#region Page2*/\n\n/*#endregion Page2*/\n\n/*#region Page3*/\n\n/*#endregion Page3*/\n\n/*#region Page4*/\n\n/*#endregion Page4*/\n\n/*#region Footer*/\n\n/*#endregion Footer*/\n"},"lineCount":null}},"error":null,"hash":"b7e797b13017a3a72de8590fa4f6e4e0","cacheData":{"env":{}}}